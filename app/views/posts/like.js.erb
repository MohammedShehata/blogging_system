$("#like_form").attr("action", "<%= unlike_path(@post)%>");
$("#like_txt").text("Unlike");
$("#like_icon").addClass("icon-thumbs-down").removeClass("icon-thumbs-up");

$("<span/>", {
	id: "u-<%= current_user.id %>"
}).appendTo("#like_by");
$("<a/>",{
	href: "<%=profile_path(current_user)%>",
	text: "<%= current_user.username %>"
}).appendTo("#u-<%= current_user.id %>");

